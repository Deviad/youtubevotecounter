{
    "compilerOptions": {
        "outDir": "./dist/",
        "baseUrl": ".",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es2020",
        "allowJs": true,
        "esModuleInterop": true,
        "strict": true,
        "strictNullChecks": true,
        "alwaysStrict": true,
        "paths": {
            "typings/*": ["./typings/*"]
        }
    }
}
